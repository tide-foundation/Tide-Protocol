{

  "build": {
    "content": [
      {
        "files": ["docs/**.md", "docs/**/toc.yml", "toc.yml", "*.md"]
      }
    ],
    "resource": [{
      "files": ["images/**"]
    }],
    "overwrite": [{
      "files": ["apidoc/**.md"],
      "exclude": ["obj/**", "build/**"]
    }],

    "dest": "build",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": ["default", "templates/tide"],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "globalMetadata": {
      "_appTitle": "Tide Documentation",
      "_disableBreadcrumb": false
    }
  }
}