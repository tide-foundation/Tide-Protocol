{
  "metadata": [{
      "src": [{
        "src": "../../",
        "files": ["Tide.VendorSdk/**.csproj"],
        "exclude": ["**/bin/**", "**/obj/**"]
      }],
      "dest": "api/vendor-sdk"
    },
    {
      "src": [{
        "src": "../../",
        "files": ["Tide.Ork/**.csproj"],
        "exclude": ["**/bin/**", "**/obj/**"]
      }],
      "dest": "api/ork"
    }
  ],
  "build": {
    "content": [{
        "files": ["api/vendor-sdk/**.yml", "api/ork/**.yml", "api/tide-js/**.yml", "api/index.md"]
      },
      {
        "files": ["docs/**.md", "docs/**/toc.yml", "toc.yml", "*.md"]
      }
    ],
    "resource": [{
      "files": ["images/**"]
    }],
    "overwrite": [{
      "files": ["apidoc/**.md"],
      "exclude": ["obj/**", "build/**"]
    }],

    "dest": "build",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": ["default", "templates/tide"],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "globalMetadata": {
      "_appTitle": "Tide Documentation",
      "_disableBreadcrumb": false
    }
  }
}