{
  "metadata": [
    {
      "src": [
        {
          "files": ["**.csproj"],
          "src": "../Tide.Js/"
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [ "article/**.yml", "article/index.md" ]
      },
      {
        "files": [ "article/**.md", "article/**/toc.yml", "toc.yml", "*.md" ]
      },
      {
        "files": [ "article/**.md", "article/**/tide.yml", "tide.yml", "*.md" ]
      }
      
    ],
    "resource": [
      {
        "files": ["images/**"]
      }
    ],
    "overwrite": [
      {
        "files": ["apidoc/**.md"],
        "exclude": ["obj/**", "_site/**"]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": ["default"],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}
